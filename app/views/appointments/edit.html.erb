
<div class="container-fluid">
<%= form_for @appointment,:url=>appointment_path,:method=>:post do |f|%>
    <div class="form-group", class="col-xs-4">
     <%= f.label :doctor_name%>
     <%= f.text_field :doctor_id%>
    </div>


    <div class="form-group", class="col-xs-4">
      <%= f.label :patient_name%>
     <%= f.text_field :patient_id%>
    </div>

    <div class="form-group", class="col-xs-4">
      <%= f.label :appointment_date%>
     <%= f.text_field :appointment_date%>
    </div>
    
<button class="btn btn-primary " name="submit" type="submit">
        Submit
       </button>
    </div> 
 <%end%>
</div>  